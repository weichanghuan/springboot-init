pipeline {
    agent any

    stages {
        stage('Build JAR') {
            steps {
                sh "mvn clean package -B -e -U -DskipTests"
            }
        }

        stage('Build Docker Image') {
                        steps {
                            // 该步骤通常不应该在您的脚本中使用。请参考帮助查看详情。
                            withDockerRegistry(credentialsId: '11111', url: 'https://hub.docker.com/repository/docker/weichanghuan/springboot-init') {
                                sh "docker build -f ./springboot-init/src/main/docker/Dockerfile -t weichanghuan/springboot-init:v1 ."
                            }
                        }
        }
    }



}